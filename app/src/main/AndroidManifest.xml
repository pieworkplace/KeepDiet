<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.keepdiet.android.keepdiet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_fitness_center_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_fitness_center_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserTargetActivity" />
        <activity android:name=".startGroup" />
        <activity android:name=".setTarget" />
        <activity android:name=".FindGroup" />
        <activity android:name=".FindGroupResult" />
        <activity android:name=".GroupInformation"></activity>
    </application>

</manifest>